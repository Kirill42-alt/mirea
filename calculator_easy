#include <stdio.h>
#include <conio.h>
#include <math.h>

int main (void) {

    double a, b;
    char x = '+'; char y = '-'; char v = '*'; char z = '/'; char t = '^'; char result;
    
    do {
        scanf("%lf%c%lf", &a, &result, &b);
        
        if (result == x) {
            printf("%lf + %lf = %lf\n", a, b, a + b);
        } else if (result == y) {
            printf("%lf - %lf = %lf\n", a, b, a - b);
        } else if (result == v) {
            printf("%lf * %lf = %lf\n", a, b, a * b);
        } else if (result == z) {
            printf("%lf / %lf = %lf\n", a, b, a / b);
        } else if (result == t) {
            printf("%lf ^ %lf = %lf\n", a, b, pow(a, b));
        } else {printf("Непонятное выражение.\n\n");}
    
    } while (1);}
